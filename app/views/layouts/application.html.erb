  <%= csrf_meta_tags %>



</head>
<body>



    <div class="row etrow3">
        <div id="et_logo_area">
            <img src="http://imagizer.imageshack.us/v2/280x200q90/911/BIaHOP.png" id="et_sbs_logo">
        </div>
        <div class="col-sm-12" id="et_main">
            <div id="et_page_content">
                <object>
                    <embed id="ett" />
                </object>
                <script>
                    var description = [
                        "/18s_l_8.svg",
                        "/18s_l_6.svg",

                        "/18s_l_10.svg",
                        "/18s_l_11.svg",
                        "/18s_l_12.svg",
                        "/18s_l_13.svg",
                        "/18s_l_14.svg",
                        "/18s_l_4.svg"];
                    var size = description.length
                    var x = Math.floor(size * Math.random())
                     document.getElementById('ett').src = description[x];
                </script>
                
            </div>
        </div>
        <div id="et_icons_control">
            <ul>
                <li>
                    <object>
                        <embed src="/18s_home.svg">
                    </object>
                </li>
                <li>
                    <object>
                        <embed src="/18s_intro_f.svg">
                    </object>
                </li>
                <li>
                    <object>
                        <embed src="/18s_part.svg">
                    </object>
                </li>
                <li>
                    <object>
                        <embed src="/18s_movie.svg">
                    </object>
                </li>
                <li>
                    <object>
                        <embed src="/18s_mc_ff.svg">
                    </object>
                </li>
            </ul>
            <br><br>
        </div>
    </div>
<%= yield %>

    <!-- FOOTER -->
    <footer>
      <p class="pull-right"><a href="#"></a>
      </p>
      <p>
          <img src="http://www.usdirect.com/images/new.usdirect/logo/logo-sbs.png">
      </p>
    </footer>

    <script type="text/javascript" src="/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="/js/lightbox.min.js"></script>
    <script type="text/javascript" src="/js/owl.carousel.min.js"></script>
    <script type="text/javascript" src="/js/jquery.countTo.js"></script>
    <script type="text/javascript" src="/js/jquery.appear.js"></script>
    <script type="text/javascript" src="/js/SmoothScroll.js"></script>
    <script type="text/javascript" src="/js/isotope.pkgd.min.js"></script>
    <script type="text/javascript" src="/js/jquery.localScroll.min.js"></script>
